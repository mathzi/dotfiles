[user]
	name = Matheus Ferarezi
	email = matheusferarezi@gmail.com
[apply]
    	whitespace = nowarn
[color]
    	diff = auto
    	status = auto
    	branch = auto
    	ui = true
[help]
    	autocorrect = 1
[core]
	editor = subl -n -w
    	excludesfile = ~/.gitignore
[alias]
	co = commit -am
	p = push origin master
	s = status
	b = branch
	ck = checkout
	d = diff
